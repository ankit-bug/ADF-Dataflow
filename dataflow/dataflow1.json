{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "Ls_blobsa",
						"type": "LinkedServiceReference"
					},
					"name": "SourceData"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_GENDEST",
						"type": "DatasetReference"
					},
					"name": "sinkdata"
				}
			],
			"transformations": [
				{
					"name": "filtertherows"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Country as string,",
				"          Year as short,",
				"          Product_Type as string,",
				"          Sales as short",
				"     ),",
				"     useSchema: false,",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delimited',",
				"     container: 'destsa12',",
				"     fileName: 'Sales_File_2014_2019_USA_UK_IND.csv',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true) ~> SourceData",
				"SourceData filter(Year == 2014 && Product_Type == 'electronics') ~> filtertherows",
				"filtertherows sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkdata"
			]
		}
	}
}